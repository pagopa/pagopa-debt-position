            "schema": {
              "type": "string"
            }
          },
          {
            "name": "nav",
            "in": "path",
            "description": "NAV (notice number) is the unique reference assigned to the payment by a creditor institution.",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
